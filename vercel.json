{
  "crons": [
    {
      "path": "/api/cache-warm-cron",
      "schedule": "*/10 * * * *"
    }
  ],
  "functions": {
    "src/pages/api/*.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, stale-while-revalidate=600"
        }
      ]
    }
  ]
}
